{
  "name": "@agentgate/cli",
  "version": "0.0.1",
  "type": "module",
  "description": "CLI for AgentGate approval management",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentgate": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "files": ["dist"],
  "dependencies": {
    "@agentgate/core": "workspace:*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "vitest": "^2.1.8"
  }
}
