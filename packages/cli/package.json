{
  "name": "@agentgate/cli",
  "version": "0.0.1",
  "description": "CLI for AgentGate approval management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentgate": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amitpaz1/agentgate.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "agentgate",
    "approval",
    "agent",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "@agentgate/core": "workspace:*",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.12.0",
    "vitest": "^2.1.8"
  }
}
