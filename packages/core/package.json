{
  "name": "@agentgate/core",
  "version": "0.0.1",
  "description": "Core types and utilities for AgentGate",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/amitpaz1/agentgate.git",
    "directory": "packages/core"
  },
  "keywords": [
    "agentgate",
    "approval",
    "agent",
    "types"
  ],
  "license": "MIT",
  "devDependencies": {
    "vitest": "^2.1.8"
  }
}
